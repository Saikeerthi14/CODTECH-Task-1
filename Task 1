
num1=int(input("enter first number:"))
num2=int(input("enter second number:"))
ch=input("enter any operations +,-,*,/ :")
result=0
if ch=='+':
    result=num1+num2
elif ch=='-':
    result=num1-num2
elif ch=='*':
    result=num1*num2
elif ch=='/':
    result=num1/num2
else:
    print("invalid operator")
print(num1, ch, num2, ":", result)

    
